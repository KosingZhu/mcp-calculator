{
  "mcpServers": {
    "local-stdio-calculator": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "calculator"]
    },
    "local-operator": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "pc_operator"]
    },
    "remote-sse-server": {
      "type": "sse",
      "url": "https://api.example.com/sse",
      "disabled": true
    },
    "remote-http-server": {
      "type": "http",
      "url": "https://api.example.com/mcp",
      "disabled": true
    },
    "Playwright": {
      "type": "http",
      "url": "http://localhost:30045/mcp",
      "disabled": false
    },
    "Playwright_npx": {
      "type": "stdio",
      "command": "npx.cmd",
      "args": ["-y", "@playwright/mcp@latest"]
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "E:\\AILearning"
      ]
    },
    "mcp-server-git": {
      "type": "stdio",
      "command": "uvx",
      "args": ["mcp-server-git"]
    }
  }
}
