{
  "mcpServers": {
    "local-stdio-calculator": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "calculator"]
    },
    "local-operator": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "pc_operator"]
    },
    "remote-sse-server": {
      "type": "sse",
      "url": "https://api.example.com/sse",
      "disabled": true
    },
    "remote-http-server": {
      "type": "http",
      "url": "https://api.example.com/mcp",
      "disabled": true
    },
    "Playwright": {
      "type": "http",
      "url": "http://localhost:30045/mcp",
      "disabled": true
    },
    "Playwright_npx": {
      "type": "stdiobak禁用",
      "command": "npx.cmd",
      "args": ["-y", "@playwright/mcp@latest"],
      "disabled": true
    },
    "streamable-mcp-server": {
      "type": "streamablehttp",
      "url": "http://127.0.0.1:12306/mcp",
      "disabled": false
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "E:\\AILearning"
      ]
    },
    "mcp-server-git": {
      "type": "stdio",
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "memory": {
      "type": "stdio",
      "command": "npx.cmd",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "E:\\AILearning\\atomS3R\\xiaozhimcp\\memory.json"
      }
    },
    "excalidraw": {
      "command": "node",
      "args": [
        "E:\\AILearning\\atomS3R\\mcp_excalidraw\\dist\\index.js"
      ]
    }
  }
}
